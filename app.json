{
  "expo": {
    "name": "VScan",
    "slug": "VScan",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon2.gif",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "backgroundColor": "#000000",
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon2.gif",
        "backgroundColor": "#000000"
      },
      "package": "com.anonymous.VScan"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon2.gif",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000000"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
